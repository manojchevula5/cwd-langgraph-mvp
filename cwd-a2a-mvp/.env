# Redis configuration
REDIS_URL=redis://localhost:6379/0

# Agent URLs
COORDINATOR_URL=http://localhost:8001
DELEGATOR_URL=http://localhost:8002
WORKER_URL=http://localhost:8003

# LLM provider (stub, openai, gemini)
LLM_PROVIDER=stub

# Optional: LLM API keys (set when LLM_PROVIDER != stub)
# OPENAI_API_KEY=
# GEMINI_API_KEY=

# Logging level
LOG_LEVEL=INFO

# Azure Databricks Configuration
# Rename this file to .env or add these values to your existing .env file

# The URL of your Azure Databricks workspace (e.g., https://adb-123456789.12.azuredatabricks.net)
DATABRICKS_HOST=https://adb-<workspace-id>.<region>.azuredatabricks.net

# Your Personal Access Token (Settings -> Developer -> Access Tokens)
DATABRICKS_TOKEN=dapi...

# This tells MLflow to use the Databricks logging configuration
MLFLOW_TRACKING_URI=databricks
